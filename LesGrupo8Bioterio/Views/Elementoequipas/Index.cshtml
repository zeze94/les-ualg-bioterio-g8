@model IEnumerable<LesGrupo8Bioterio.Elementoequipa>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Equipa(ElementoEquipa)</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nome)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Função)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FuncionarioIdFuncionarioNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProjetoIdProjetoNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(model => item.Nome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Função)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FuncionarioIdFuncionarioNavigation.NomeCompleto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProjetoIdProjetoNavigation.Nome)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdElementoEquipa"><span class="glyphicon glyphicon-pencil"></span></a> |
                <a asp-action="Details" asp-route-id="@item.IdElementoEquipa"><span class="glyphicon glyphicon-eye-open"></span></a> |
                <a asp-action="Delete" asp-route-id="@item.IdElementoEquipa"><span class="glyphicon glyphicon-trash"></span></a>
            </td>
        </tr>
}
    </tbody>
</table>
