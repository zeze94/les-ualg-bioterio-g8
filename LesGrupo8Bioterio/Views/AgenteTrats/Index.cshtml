@model IEnumerable<LesGrupo8Bioterio.AgenteTrat>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>Agentes de Tratamento</h2>
<p>
    <a type="button" class="btn btn-info" asp-action="Create">Criar Novo</a>
</p>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NomeAgenTra)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.NomeAgenTra)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.IdAgenTra"><span class="glyphicon glyphicon-pencil"></span></a> |
                    <a asp-action="Details" asp-route-id="@item.IdAgenTra"><span class="glyphicon glyphicon-eye-open"></span></a> |
                    <a asp-action="Delete" asp-route-id="@item.IdAgenTra"><span class="glyphicon glyphicon-trash"></span></a>
                </td>
            </tr>
        }
    </tbody>
</table>


<div class="col-md-12 col-md-push-1">
    <a type="button" class="btn btn-info pull-right" asp-controller="RegTratamentoes" asp-action="Index">Voltar Para Tratamentos</a>
</div>
