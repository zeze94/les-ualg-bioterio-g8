@model LesGrupo8Bioterio.Tanque

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Detalhes do Tanque</h2>

<div class="table-bordered" >
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.NroAnimais)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.NroAnimais)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Sala)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Sala)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Observacoes)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Observacoes)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.CircuitoTanqueIdCircuitoNavigation)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.CircuitoTanqueIdCircuitoNavigation.IdCircuito)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.LoteIdLoteNavigation)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.LoteIdLoteNavigation.CodigoLote)
        </dd>
    </dl>
    <div class="form-group">
        <a type="button" class="btn btn-info" asp-action="Edit" asp-route-id="@Model.IdTanque">Editar</a>
        <a type="button" class="btn btn-danger" asp-action="Delete" asp-route-id="@Model.IdTanque">Eliminar</a>
        <a type="button" class="btn btn-info" asp-action="Index">Cancelar</a>
